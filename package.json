{
  "name": "cigar-lounges-luxury-premium",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 4000",
    "build": "next build",
    "start": "next start -p 4000",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "analyze": "ANALYZE=true npm run build",
    "lighthouse": "lighthouse http://localhost:4000 --output html --output-path ./lighthouse-report.html",
    "lighthouse:ci": "lhci autorun",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:accessibility": "pa11y http://localhost:4000",
    "test:performance": "npm run lighthouse && npm run test:accessibility",
    "precommit": "lint-staged",
    "prepare": "husky install",
    "optimize-images": "next-optimized-images",
    "sitemap": "next-sitemap",
    "security-audit": "npm audit",
    "security-audit:fix": "npm audit fix",
    "deps:update": "npm update",
    "deps:check": "npm outdated",
    "clean": "rm -rf .next node_modules/.cache",
    "clean:all": "rm -rf .next node_modules package-lock.json",
    "reinstall": "npm run clean:all && npm install",
    "build:analyze": "npm run build && npx @next/bundle-analyzer .next",
    "preview": "npm run build && npm run start"
  },
  "dependencies": {
    "next": "15.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.3.3",
    "framer-motion": "^11.0.0",
    "@react-three/fiber": "^8.15.0",
    "@react-three/drei": "^9.95.0",
    "three": "^0.160.0",
    "gsap": "^3.12.2",
    "lenis": "^1.0.42",
    "clsx": "^2.1.0",
    "react-intersection-observer": "^9.5.3",
    "@vercel/analytics": "^1.2.0",
    "sharp": "^0.33.1",
    "isomorphic-dompurify": "^2.8.0",
    "web-vitals": "^3.5.0",
    "@sentry/nextjs": "^8.0.0",
    "react-datepicker": "^4.25.0",
    "date-fns": "^3.3.1",
    "canvas-confetti": "^1.9.2",
    "mapbox-gl": "^3.0.1",
    "react-intersection-observer": "^9.5.3",
    "react-use": "^17.4.2",
    "lodash.debounce": "^4.0.8",
    "lodash.throttle": "^4.1.1",
    "lucide-react": "^0.263.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/three": "^0.160.0",
    "@types/lodash.debounce": "^4.0.9",
    "@types/lodash.throttle": "^4.1.9",
    "@types/canvas-confetti": "^1.6.4",
    "eslint": "^8.56.0",
    "eslint-config-next": "15.5.3",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-import": "^2.29.1",
    "prettier": "^3.1.1",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "tailwindcss": "^3.4.0",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "jest": "^29.7.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/jest-dom": "^6.1.6",
    "@testing-library/user-event": "^14.5.1",
    "jest-environment-jsdom": "^29.7.0",
    "playwright": "^1.40.1",
    "@playwright/test": "^1.40.1",
    "pa11y": "^7.0.0",
    "lighthouse": "^11.4.0",
    "@lhci/cli": "^0.11.0",
    "bundle-analyzer": "^0.0.3",
    "@next/bundle-analyzer": "^14.0.4",
    "next-sitemap": "^4.2.3",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "cross-env": "^7.0.3",
    "rimraf": "^5.0.5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,css}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run type-check && npm run lint"
    }
  },
  "engines": {
    "node": ">=18.17.0",
    "npm": ">=9.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}